<h1>Listing Student Names</h1>
<%= link_to 'New Students', new_student_path %>
<table>
  <thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th colspan="3">Action</th>
  </tr>
  </thead>
  <tbody>
  <% @students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.email %></td>
      <td><%= link_to 'View', student %></td>
      <td><%= link_to 'Edit ', edit_student_path(student) %></td>
      <td> <%= link_to 'Delete', '#', class: 'delete-link',
                       data: { confirm: 'Are you sure you want to delete?' },
                       "data-url": student_path(student) %></td>
    </tr>
  <% end %>
  </tbody>
</table>